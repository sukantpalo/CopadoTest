    <validationRules>
        <fullName>skuid__Max_Auto_Saves_At_Least_0</fullName>
        <active>true</active>
        <description>Max Auto-Saves must be greater or equal to 0</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(skuid__Max_Auto_Saves__c)),
skuid__Max_Auto_Saves__c &lt; 0
)</errorConditionFormula>
        <errorDisplayField>skuid__Max_Auto_Saves__c</errorDisplayField>
